---
name: streaming
graphs:
  start:
    - type: RegexNode
      content: "x"
      regex: "x"
      output: output
      groups:

    - type: MapNode
      list: "{output}"
      mapTarget: D

  D:
    - type: EchoNode
      put: "gid"
      value: "{{element}@gid}"

    - type: StringContainsNode
      regex: "notimportant"
      content: "{gid}" # content :: String
