---
name: forkjoin
graphs:
  start:
    - type: ForkJoin
      forkTargets: [A,B]
      targetToKeys:
        A: { keyfroma: keyfroma }
        B: { keyfromb: keyfromb }

    - type: StringContains
      regex: ""
      content: "{keyfroma}" # content :: String

  A:
    - type: Echo
      put: keyfroma
      value: "1"

  B:
    - type: Echo
      put: keyfromb
      value: "1"


